import java.util.Scanner;
 class main {
    public static int demchu(String dem) {
        String[] words = dem.trim().split("\\s+");
        return words.length;
    }
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Nhap chu: ");
        String dem = scanner.nextLine();
        System.out.println("so chu: " + demchu(dem));
        scanner.close();
    }}
